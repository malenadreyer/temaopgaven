---

import BaseLayout from "../layouts/BaseLayout.astro";
import Header from "../components/Header.astro"
import Wte from "../components/Wte.astro";
import Scrollingcontainer from "../components/Scrollingcontainer.astro";
import Animation from "../components/Animation.astro";
import CoreValues from "../components/CoreValues.astro";
import ContactFr from "../components/ContactFr.astro";

const employeeData = await fetch(
  "https://raw.githubusercontent.com/daviatkea/frontend-design/main/resources/json/employees.json"
).then((res) => res.json());

const coreValuesData = await fetch(
  "https://raw.githubusercontent.com/daviatkea/frontend-design/main/resources/json/corevalues.json"
).then((res) => res.json());

const firstThreeEmployees = employeeData.slice(0, 3);
---



<BaseLayout title ="Home">
    <Header/>
    <Wte/>
   <Scrollingcontainer/>
   <Animation/>
   <CoreValues 
   bgColor="var(--color-black)" 
   textColor="var(--color-white)"
   knapColor="var(--color-white)"
   knapSkrift="var(--color-black)"/>
<ContactFr/>
</BaseLayout>
